ErrorDocument 404 /404/index.php

RewriteEngine On
RewriteCond %{HTTP_HOST} !^localhost$ [NC]
RewriteCond %{HTTP_HOST} !^mrpaint.dyndns.info$ [NC] 
RewriteCond %{HTTP_HOST} !^ponology.com$ [NC] 
RewriteCond %{HTTP_HOST} !^chaocovietnam.net$ [NC] 
RewriteRule ^(.*)$ http://chaocovietnam.net/$1 [L,R=301]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)\.htm$ index.php?$1&%{QUERY_STRING} [L]